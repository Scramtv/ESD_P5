clc
clear all
close all

load("matlabdata.mat")

temp11 = sqrt((Meas511.RE).^2 + (Meas511.IM).^2);
s_para11 = 20*log10(temp11);

figure
hold on
plot(Sim5c.x, Sim5c.y, DisplayName= 'Simulated')
plot(Meas510.Freq/(10^9), Meas510.RE, DisplayName="510")
plot(Meas511.Freq/(10^9), s_para11, DisplayName="511")

grid on
ylabel("S-11 [dB]")
xlabel("Frequency [GHz]");
xlim([2.3 2.])
legend show;
title("Simulation compared to measurement of Antenna 5.1.0 and 5.1.1")
hold off